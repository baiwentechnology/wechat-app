<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baiwen.business.mapper.IOperationFlowMapper">
    <resultMap id="BaseResultMap" type="com.baiwen.business.model.OperationFlow">
        <id column="flow_id" jdbcType="INTEGER" property="flowId"/>
        <id column="user_id" jdbcType="INTEGER" property="userId"/>
        <id column="business_id" jdbcType="INTEGER" property="businessId"/>
        <id column="goods_id" jdbcType="INTEGER" property="goodsId"/>
        <id column="user_goods_id" jdbcType="INTEGER" property="userGoodsId"/>
        <id column="flow_type" jdbcType="INTEGER" property="flowType"/>
        <id column="content" jdbcType="VARCHAR" property="content"/>
        <id column="create_time" jdbcType="DATE" property="createTime"/>
    </resultMap>

    <insert id="addOperationFlow" parameterType="com.baiwen.business.model.OperationFlow">
        insert into operation_flow (user_id,business_id,goods_id,user_goods_id,flow_type,content,create_time)
        values (#{userId},#{businessId},#{goodsId},#{userGoodsId},#{flowType},#{content},now())
    </insert>

</mapper>