<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baiwen.business.mapper.IsysMapper">
    <resultMap id="BaseResultMap" type="com.baiwen.business.model.SysConfig">
        <id column="id" jdbcType="BIGINT" property="id"/>
        <id column="config_type" jdbcType="BIGINT" property="configType"/>
        <id column="config_value" jdbcType="BIGINT" property="configValue"/>
    </resultMap>
    <select id="getSysConfigById" resultMap="BaseResultMap">
        select * from sys_config where config_type =#{configType}
    </select>
</mapper>